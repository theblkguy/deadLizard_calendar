{
  "name": "deadlizard-calendar",
  "version": "1.0.0",
  "description": "Studio booking calendar application with role-based access",
  "main": "server/dist/server.js",
  "scripts": {
    "dev": "npm run build:dev && cd server && npm run dev",
    "dev:watch": "concurrently \"npm run client:watch\" \"cd server && npm run dev\"",
    "client:watch": "webpack --config webpack.dev.js --watch",
    "server:dev": "cd server && npm run dev",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build:server && npm run build",
    "start": "npm run build:all && cd server && npm run start",
    "start:dev": "npm run build:dev && cd server && npm run dev",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "install-server": "cd server && npm install",
    "install-client": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "keywords": [
    "mern",
    "calendar",
    "booking",
    "studio"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
